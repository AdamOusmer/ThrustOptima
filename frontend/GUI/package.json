{
  "name": "thrustoptima",
  "version": "0.0.0",
  "main": "main.js",
  "scripts": {
    "start": "node_modules/.bin/electron .",
    "build": "npm run build-arm64 && npm run build-x64 && npm run build-linux && npm run build-windows",
    "build-arm64": "node_modules/.bin/electron-packager . --platform=darwin --arch=arm64 --overwrite --out=../../out/arm64 --icon=assets/img/icons/ThrustOptima_Logo.incs",
    "build-x64": "node_modules/.bin/electron-packager . --platform=darwin --arch=universal --overwrite --out=../../out/x64 --icon=assets/img/icons/ThrustOptima_Logo.incs",
    "build-windows": "node_modules/.bin/electron-packager . --platform=win32 --arch=x64 --overwrite --out=../../out/win --icon=assets/img/ThrustOptima_Logo.png",
    "build-linux": "node_modules/.bin/electron-packager . --platform=linux --arch=x64 --overwrite --out=../../out/linux --icon=assets/img/icons/ThrustOptima_Logo.incs"
  },
  "build": {
    "appId": "me.adam-ousmer.thrust-optima",
    "asar": true,
    "productName": "Thrust Optima",
    "directories": {
      "output": "../../out/win"
    },
    "win": {
      "icon": "assets/img/icons/ThrustOptima_Logo.ico"
    }
  },
  "description": "Thrust Optima is a tool for optimizing the performance of a rocket engine.",
  "author": "Adam Ousmer",
  "company": "Space Concordia - Rocketry Division",
  "devDependencies": {
    "electron": "^26.2.4",
    "electron-packager": "^17.1.2"
  }
}
